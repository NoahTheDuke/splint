#kaocha/v1
#meta-merge
[{:tests [{:kaocha.testable/id :all
           :kaocha.testable/type :kaocha.type/clojure.test
           :kaocha/ns-patterns [".*"]
           :kaocha/source-paths ["src" "dev"]
           :kaocha/test-paths ["test"]}]
  :color? true
  :fail-fast? false
  :reporter kaocha.report.progress/report
  :plugins ^:replace [:kaocha.plugin/capture-output
                      :kaocha.plugin/filter
                      :kaocha.plugin/print-invocations
                      :kaocha.plugin/randomize]
  :kaocha.plugin.capture-output/capture-output? true
  :kaocha.plugin.randomize/randomize? true}
 #include "tests-user.edn"]
